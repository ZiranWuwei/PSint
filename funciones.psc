funcion suma<-sumar(num1,num2)
	definir suma como entero;
	suma<-num1+num2;
FinFuncion

Proceso sin_titulo
	definir num1,num2 como enteros;
	escribir Sin Saltar "Digite un numero: ";	
	leer num1;
	Escribir Sin Saltar "Digite un segundo numero: ";
	leer num2;
	
	escribir "La suma de ambos numeros es: ",sumar(num1,num2);
	
	
FinProceso
